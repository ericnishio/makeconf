    "prompt": "^0.2.14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ericnishio/makeconf.git"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "eslint": "^1.4.1",
    "mocha": "^2.3.2",
    "sinon": "^1.16.1"
  }
