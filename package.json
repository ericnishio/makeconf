{
  "name": "makeconf",
  "version": "1.4.3",
  "description": "An interactive local config generator for better team coordination.",
  "main": "index.js",
  "engine-strict": true,
  "engines": {
    "node": ">=4.0.0"
  },
  "bin": {
    "makeconf": "bin/makeconf"
  },
  "preferGlobal": "true",
  "scripts": {
    "test": "node node_modules/mocha/bin/mocha",
    "lint": "node node_modules/eslint/bin/eslint.js index.js lib",
